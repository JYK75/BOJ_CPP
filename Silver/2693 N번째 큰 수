#include <bits/stdc++.h>

int main()
{
	std::ios::sync_with_stdio(0);
	std::cin.tie(0);

	int T;
	std::cin >> T;
	while (T--)
	{
		int num;
		int arr[10];
		for (int i = 0; i < 10; ++i)
		{
			std::cin >> num;
			arr[i] = num;
		}
		std::sort(arr, arr + 10);
		
		std::cout << arr[7] << '\n';
	}
	return 0;
}
