#include <bits/stdc++.h>

int main()
{
	std::ios::sync_with_stdio(0);
	std::cin.tie(0);
	
	int arr[5];
	for (int i = 0; i < 5; ++i)
	{
		std::cin >> arr[i];
		if (arr[i] < 40)
			arr[i] = 40;
	}

	int result = 0;
	for (int i = 0; i < 5; ++i)
	{
		result += arr[i];
	}

	std::cout << result / 5;

	return 0;
}
