#include <bits/stdc++.h>

int main()
{
	int arr[301][301];

	int n, m;
	std::cin >> n >> m;

	for (int i = 1; i <= n; ++i)
	{
		for (int j = 1; j <= m; ++j)
		{
			std::cin >> arr[i][j];
		}
	}

	int K;
	std::cin >> K;
	
	while (K--)
	{
		int sum = 0;

		int x1, y1, x2, y2;
		std::cin >> x1 >> y1 >> x2 >> y2;

		for (int i = x1; i <= x2; ++i)
		{
			for (int j = y1; j <ã… = y2; ++j)
			{
				sum += arr[i][j];
			}
		}

		std::cout << sum << std::endl;
	}

	return 0;
}
